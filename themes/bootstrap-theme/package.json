{
  "name": "bootstrap-theme",
  "scripts": {
    "build": "node-sass --include-path node_modules theme.scss static/css/theme.css",
    "watch": "node-sass --watch --include-path node_modules theme.scss static/css/theme.css"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "node-sass": "^4.10.0",
    "npm-watch": "^0.5.0",
    "popper.js": "^1.14.3"
  }
}
